#include <stdio.h>
#include <stdlib.h>
#include "my_rpg.h"

int add_arrow(dll_t *list)
{
	object_t *arrow = malloc(sizeof(*arrow));

	if (arrow == NULL)
		return (84);
	arrow->pos.x = 890;
	arrow->pos.y = 460;
	arrow->rect.height = 65;
	arrow->rect.left = 2082;
	arrow->rect.top = 2202;
	arrow->rect.width = 65;
	put_end_list(list, arrow);
	return (0);
}
